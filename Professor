CREATE TABLE Professor (
	matricula decimal(15),
    nome text,
    lotacao smallint,
    titulacao varchar(30),
    categoria varchar(30),
    dataAdmissao decimal(8),
    cargaHoraria int,
    cargaHAlocada int
);


